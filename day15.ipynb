   "source": [
    "def greet(name):\n",
    "    return f\"Hello, {name}\"\n",
    "\n",
    "# Assigning function to a variable\n",
    "say_hello = greet\n",
    "print(say_hello(\"Eeesa\"))   # Hello, Eeesa\n",
    "\n",
    "# Passing function as argument\n",
    "def executor(func, value):\n",
    "    return func(value)\n",
    "\n",
    "print(executor(greet, \"Python\"))  # Hello, Python\n"
   ]
  }
